{
  "name": "demo-browserify",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "install-bin": "npm install -g coffee gulp",
    "clean": "coffee -c gulpfile.coffee && gulp clean",
    "build-dev": "npm install && coffee -c gulpfile.coffee && gulp clean && gulp build-dev",
    "build-prod": "npm install && coffee -c gulpfile.coffee && gulp clean && gulp build-prod",
    "quick-start": "coffee -c gulpfile.coffee && gulp clean && gulp build-dev && node ./dist/index.js",
    "quick-debug": "coffee -c gulpfile.coffee && gulp clean && gulp build-dev && node-debug ./dist/index.js",
    "start": "npm run build-prod && npm start",
    "debug": "npm run build-prod && node-debug ./dist/index.js"
  },
  "dependencies": {
    "body-parser": "~1.15.2",
    "coffee-script": "~1.3.3",
    "common": "https://github.com/Jtutzo/common.git",
    "cookie-parser": "~1.4.3",
    "debug": "~2.2.0",
    "ejs": "~2.5.2",
    "express": "~4.14.0",
    "gulp": "^3.9.1",
    "gulp-browserify": "^0.5.1",
    "gulp-clean": "^0.3.2",
    "gulp-coffee": "^2.3.3",
    "gulp-concat": "^2.6.1",
    "gulp-minify-css": "^1.2.4",
    "gulp-rename": "^1.2.2",
    "gulp-util": "^3.0.8",
    "jquery": "^3.1.1",
    "morgan": "~1.7.0",
    "sequelize": "^3.23.6",
    "sequelize-cli": "^2.4.0",
    "serve-favicon": "~2.3.0",
    "vinyl-source-stream": "^1.1.0"
  }
}
